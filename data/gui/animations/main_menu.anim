<?xml version="1.0" ?>
<Animations>

	<AnimationDefinition name="ConnectionProgress" duration="2.2" replayMode="loop" autoStart="true">
		<Affector property="CurrentProgress" interpolator="float">
			<KeyFrame position="0" value="0.0" />
			<KeyFrame position="0.9" value="0.9" progression="linear" />
			<KeyFrame position="1.5" value="0.3" progression="linear" />
			<KeyFrame position="2.2" value="1.0" progression="linear" />
		</Affector>
	</AnimationDefinition>

</Animations>
